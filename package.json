{
  "name": "callertech-desktop-phone",
  "appId": "com.callertech.webphone",
  "productName": "CallerTech Desktop Phone",
  "author": "callertech",
  "version": "1.4.4",
  "license": "MIT",
  "description": "Desktop app for calling using CallerTech phone numbers.",
  "main": "public/electron.js",
  "keywords": [
    "Electron",
    "React",
    "Webpack",
    "Babel"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=dev yarn run config && npm-run-all --parallel start:react start:electron",
    "start:beta": "cross-env NODE_ENV=staging npm-run-all --parallel config start:react start:electron",
    "start:electron": "electron .",
    "config": "node config/copy-config.js",
    "start:react": "cross-env NODE_ENV=dev node config/copy-config.js && webpack-dev-server --config ./webpack.config.js --mode development",
    "bundle:react": "webpack --mode production",
    "build:electron": "electron-builder --config ./build.js",
    "webapp": "cross-env NODE_ENV=prod yarn run bundle:react",
    "webapp:staging": "cross-env NODE_ENV=staging yarn run bundle:react",
    "release": "cross-env NODE_ENV=prod yarn run config && yarn run bundle:react && yarn run build:electron",
    "release:beta": "cross-env NODE_ENV=staging yarn run config && yarn run bundle:react && yarn run build:electron"
  },
  "devDependencies": {
    "@babel/core": "^7.16.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.3",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.7",
    "@types/facebook-js-sdk": "^3.3.2",
    "babel-loader": "^8.2.4",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "electron": "^11.0.1",
    "electron-builder": "^22.9.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "react-hot-loader": "^4.13.0",
    "react-notification-system": "^0.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@date-io/moment": "^2.11.0",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "autolinker": "^3.11.1",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "clsx": "^1.0.4",
    "custom-electron-titlebar": "^3.2.5",
    "debug": "^4.1.1",
    "electron-context-menu": "^0.16.0",
    "electron-is-dev": "^1.1.0",
    "electron-json-storage": "^4.1.8",
    "electron-updater": "^4.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.0-beta.1",
    "inline-worker": "^1.1.0",
    "json-loader": "^0.5.7",
    "jssip": "^3.7.0",
    "laravel-echo": "^1.10.0",
    "libphonenumber-js": "^1.7.26",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "msr": "^1.3.4",
    "normalizr": "^3.6.1",
    "notistack": "^0.9.6",
    "prop-types": "^15.7.2",
    "pusher-js": "^7.0.0",
    "react": "^16.6.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-chat-elements": "^10.16.0",
    "react-dom": "^16.6.0",
    "react-dropzone": "^11.2.1",
    "react-input-mask": "^2.0.4",
    "react-linkify": "^1.0.0-alpha",
    "react-redux": "^7.1.3",
    "react-swipeable-views": "^0.13.3",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.3.0"
  }
}
